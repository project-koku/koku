          {
            "name": "simulate",
            "in": "query",
            "description": "Whether to actually delete",
            "required": false,
            "schema": {
              "type": "boolean",
              "example": true
            }
          },
          {
            "name": "end_date",
            "in": "query",
            "description": "The date to end summarizing",
            "required": false,
            "schema": {
              "type": "string",
              "format": "date",
              "example": "2019-12-31"
