  "components": {
    "parameters": {
      "ProviderName": {
        "name": "provider_name",
        "in": "query",
        "description": "String to indicate the provider name.",
        "schema": {
          "type": "string",
          "example": "aws_org_tree"
        }
      }
    },
    "schemas": {
      "DownloadGetResponse": {
        "type": "object",
        "properties": {
          "Download Request Task ID": {
            "type": "string",
            "format": "uuid",
            "example": "83ee048e-3c1d-43ef-b945-108225ae52f4"
          }
        }
      },
      "EnabledTagGetResponse": {
        "type": "object",
        "properties": {
          "tag_keys": {
            "type": "array",
            "items": {"type": "string"},
            "example": ["tag1", "tag2"]
          }
        }
      },
      "EnabledTagPostResponse": {
        "type": "object",
        "properties": {
          "tag_keys": {
            "type": "array",
            "items": {"type": "string"},
            "example": ["tag1", "tag2"]
          }
        }
