{
    "catalogs": [
        {
            "user": "readonly",
            "catalog": "hive|postgres",
            "allow": "read-only"
        },
        {
            "user": "admin",
            "catalog": ".*",
            "allow": "all"
        }
    ],
    "system_session_properties": [
        {
            "role": "admin",
            "allow": true
        },
        {
            "user": "readonly",
            "allow": false
        }
    ],
    "catalog_session_properties": [
        {
            "role": "admin",
            "allow": true
        },
        {
            "user": "readonly",
            "allow": false
        }
    ],
    "system_information": [
        {
            "role": "admin",
            "allow": [
                "read",
                "write"
            ]
        },
        {
            "user": "readonly",
            "allow": [
                "read"
            ]
        }
    ]
}
