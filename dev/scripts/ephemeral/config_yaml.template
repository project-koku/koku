apps:
- name: hccm
  components:
    - name: koku
      host: %HOST%
      repo: %REPO%
      path: deploy/clowdapp.yaml
      ref: %REF%
      parameters:
        IMAGE: %IMAGE%
        IMAGE_TAG: %IMAGE_TAG%
        DBM_IMAGE: %DBM_IMAGE%
        DBM_IMAGE_TAG: %DBM_IMAGE_TAG%
        DEVELOPMENT: False
        KOKU_MIN_REPLICAS: 1
        LISTENER_MIN_REPLICAS: 1
        MASU_MIN_REPLICAS: 1
        SCHEDULER_MIN_REPLICAS: 1
        SOURCES_CLIENT_MIN_REPLICAS: 1
        SOURCES_LISTENER_MIN_REPLICAS: 1
        WORKER_CELERY_MIN_REPLICAS: 1
        WORKER_COST_MODEL_MIN_REPLICAS: 1
        WORKER_DOWNLOAD_MIN_REPLICAS: 1
        WORKER_HCS_MIN_REPLICAS: 1
        WORKER_OCP_MIN_REPLICAS: 1
        WORKER_PRIORITY_MIN_REPLICAS: 1
        WORKER_REFRESH_MIN_REPLICAS: 1
        WORKER_SUMMARY_MIN_REPLICAS: 1
        ENABLE_S3_ARCHIVING: True
        ENABLE_PARQUET_PROCESSING: False
        ENABLE_TRINO_SOURCE_TYPE: 'GCP'
        AWS_ACCESS_KEY_ID_EPH: %AWS_ACCESS_KEY_ID_EPH%
        AWS_SECRET_ACCESS_KEY_EPH: %AWS_SECRET_ACCESS_KEY_EPH%
        GCP_CREDENTIALS_EPH: 
        S3_BUCKET_NAME: koku-reports
        TRINO_DATE_STEP: 31
        PRESTO_HOST: trino-coordinator

    - name: hive-metastore
      host: github
      repo: redhatinsights/ubi-hive
      path: deploy/clowdapp.yaml
      ref: main
      parameters:
        IMAGE_TAG: %HIVE_IMAGE_TAG%
        S3_BUCKET_NAME: koku-reports
    - name: presto
      host: github
      repo: redhatinsights/ubi-trino
      path: deploy/clowdapp.yaml
      ref: main
      parameters:
        IMAGE_TAG: 360-001
        S3_BUCKET_NAME: koku-reports
        COORDINATOR_MEMORY_LIMIT: 8Gi
        WORKER_MEMORY_LIMIT: 8Gi
